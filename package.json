{
    "name": "dispatch",
    "description": "A simple blogging theme for Ghost.",
    "version": "0.0.1",
    "engines": {
        "ghost": ">=2.0.0",
        "ghost-api": "v2"
    },
    "license": "MIT",
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine",
            "Blog"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme",
        "blog"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/raykyri/dispatch.git"
    },
    "bugs": "https://github.com/raykyri/dispatch/issues",
    "config": {
        "posts_per_page": 8,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "thmb": {
                "width": 160
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    }
}
